#!/bin/sh -e
exec 2>&1
export PATH="/opt/local/sbin:/opt/local/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
<%
if File.exist?('/data/etc/environment.d/nano-envs')
  vars = File.read('/data/etc/environment.d/nano-envs').split("\n")
  vars.each do |var|
-%>
<%= var %>
<%
  end
end
-%>
    
exec <%= exec %>
